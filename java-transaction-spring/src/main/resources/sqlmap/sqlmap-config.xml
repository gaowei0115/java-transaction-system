<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 定义别名 -->
	<typeAliases>
		<typeAlias type="com.mmc.transaction.system.spring.domain.EodFileDomain" alias="eodFileDomain"/>
		<typeAlias type="com.mmc.transaction.system.spring.domain.stu.ProvinceDomain" alias="province"/>
		<typeAlias type="com.mmc.transaction.system.spring.domain.stu.CountryDomain" alias="country"/>
	</typeAliases>
	
	<!-- mapper映射 -->
	<mappers>
		<mapper resource="sqlmap/mapper/sqlmap-eodfile.xml"/>
		<mapper resource="sqlmap/mapper/sqlmap-stu-province.xml"/>
		<mapper resource="sqlmap/mapper/sqlmap-stu-city.xml"/>
		<mapper resource="sqlmap/mapper/sqlmap-stu-country.xml"/>
	</mappers>
	
	
	
</configuration>